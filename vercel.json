{
    "routes": [
      {
        "src": "/api/*",
        "dest": "/api/index.js",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/auth/login",
        "methods": ["OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "https://redux-fe.vercel.app",
          "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE",
          "Access-Control-Allow-Headers": "Content-Type"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  